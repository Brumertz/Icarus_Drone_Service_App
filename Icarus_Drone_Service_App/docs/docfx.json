{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          
          "src": ".",
          
          "files": [
            "Icarus_Drone_Service_App/Icarus_Drone_Service_App.csproj"
          ]
        }
      ],
     
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "_site/**"
        ]
      },
      {
       
        "files": [
          "api/**.yml",
          "api/**.xml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "template": [
      "default",
      "modern"
    ],
    "output": "_site",
    "globalMetadata": {
      "_appName": "IcarusDroneSite",
      "_appTitle": "IcarusDroneSite",
      "_enableSearch": true,
      "pdf": true
    }
  }
}
